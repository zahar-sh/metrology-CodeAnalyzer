<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox spacing="8.0" style="-fx-background-color: #477a7a;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="analyzer.MainWindow">
    <children>
        <TextArea fx:id="code" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="600.0" prefHeight="500.0" prefWidth="700.0" />
        <VBox spacing="8.0">
            <children>
                <ScrollPane hbarPolicy="NEVER" prefViewportHeight="500.0" prefViewportWidth="250.0" vbarPolicy="NEVER">
                    <content>
                        <Label fx:id="out" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <padding>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </padding></Label>
                    </content>
                </ScrollPane>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#analyze" text="Analyze" />
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openFile" text="Open fIle" />
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</HBox>
